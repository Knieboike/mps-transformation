<KubernetesDeploymentModel>
    <deployments>
        <deployments>
            <name>dummyDeploymentTwo</name>
            <replicas>1</replicas>
            <labels>
                <labels>
                    <key>app</key>
                    <value>dummyApp</value>
                </labels>
            </labels>
            <container>
                <container>
                    <name>dummyContainer</name>
                    <image>dummyRepo/dummy:main</image>
                    <containerPorts>
                        <containerPorts>
                            <name>containerPort</name>
                            <port>8080</port>
                        </containerPorts>
                    </containerPorts>
                    <environmentVariables>
                        <environmentVariables>
                            <key>dummyEnv</key>
                            <value>dummyEnvVal</value>
                        </environmentVariables>
                    </environmentVariables>
                </container>
            </container>
        </deployments>
        <deployments>
            <name>dummyDeployment</name>
            <replicas>3</replicas>
            <labels>
                <labels>
                    <key>app</key>
                    <value>dummyApp</value>
                </labels>
            </labels>
            <container>
                <container>
                    <name>dummyContainer</name>
                    <image>dummyRepo/dummy:main</image>
                    <containerPorts>
                        <containerPorts>
                            <name>containerPort</name>
                            <port>8080</port>
                        </containerPorts>
                    </containerPorts>
                    <environmentVariables>
                        <environmentVariables>
                            <key>dummyEnv</key>
                            <value>dummyEnvVal</value>
                        </environmentVariables>
                    </environmentVariables>
                </container>
            </container>
        </deployments>
    </deployments>
    <services>
        <services>
            <name>dummyService</name>
            <servicePorts>
                <servicePorts>
                    <name>defaultPort</name>
                    <port>80</port>
                    <targetPort>8080</targetPort>
                </servicePorts>
            </servicePorts>
            <selectors>
                <selectors>
                    <key>app</key>
                    <value>dummyApp</value>
                </selectors>
            </selectors>
        </services>
    </services>
</KubernetesDeploymentModel>